programa
{
	
	funcao inicio()
	{

		inteiro numero=0, intervalo1=0, intervalo2=0, intervalo3=0, intervalo4=0

		enquanto(numero>=0) {
     	escreva("Digite um número: ")
          leia(numero)

		se(numero>=0 e numero<=25) {
	 	intervalo1++
	 	}
		senao se(numero>=26 e  numero<=50) {
	 	intervalo2++
	 	}
		senao se(numero>=51 e numero<=75) {
	 	intervalo3++
	 	}
		senao se(numero>=76 e numero<=100) {
	 	intervalo4++
		}
	 	se(numero<0) {
	 	escreva("\nPrograma finalizado!\n")
		}
		}
		escreva("\nOs números no intervalo entre 0 e 25, foram: ",intervalo1,"\n")
		escreva("Os números no intervalo entre 26 e 50, foram: ",intervalo2,"\n")
		escreva("Os números no intervalo entre 51 e 75, foram: ",intervalo3,"\n")
		escreva("Os números no intervalo entre 76 e 100, foram: ",intervalo4,"\n")
	}
}
