	funcao inicio()
	{
		 cadeia condicao
		 inteiro peso
		 real altura, imc

		 escreva("Digite seu peso: ")
		 leia(peso)
		 escreva("Digite sua altura: ")
		 leia(altura) 

		 imc = peso / (altura*altura) 

           limpa()

		 se(imc <18.5) {
		 	escreva("Abaixo do peso")
		 }
		 senao se(imc >= 18.5 e imc < 25) {
		 	escreva("Peso normal")
		 }
		 senao se(imc >= 25 e imc < 30) {
		 	escreva("Acima do peso") 
		 }
		 senao se(imc > 30) {
		 	escreva("Obeso")
		 }
	}
}
